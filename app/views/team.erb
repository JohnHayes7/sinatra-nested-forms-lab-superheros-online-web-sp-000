<!DOCTYPE HTML>
<html>
  <head>
    <title>Display Team</title>
  </head>
  
  <body>
    <h1><%=@team.name%></h1>
    <h2><%=@team.motto%></h2>
    <%@members.each do |member|%>
      <h2><%=member.name%></h2>
      <p>
        <%=member.power%><br>
        <%=member.bio%>
      </p>
    <%end%>
  </body>
  
  </html>


